{
    "version": "1.5.2",
    "description": "Golang Version Manager",
    "homepage": "https://github.com/voidint/g",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://github.com/voidint/g/releases/download/v1.5.2/g1.5.2.windows-amd64.zip",
            "hash": "97546632cf18914f4da1b3318b4bb41eb15b49c5826802352776d705321d04d9"
        },
        "32bit": {
            "url": "https://github.com/voidint/g/releases/download/v1.5.2/g1.5.2.windows-386.zip",
            "hash": "6d9da6b9804671cd03828308d8f1109585c3f428dc02842a199a6eca2f17de20"
        }
    },
    "bin": [["g.exe", "gvm"]],
    "env_set": {
        "G_MIRROR": "https://golang.google.cn/dl/",
        "G_EXPERIMENTAL": "true",
        "G_HOME": "$dir\\.g",
        "GOROOT": "$dir\\.g\\go",
        "GOCACHE": "$dir\\.go\\GOCACHE",
        "GOENV": "$dir\\.go\\GOENV",
        "GOPATH": "$dir\\.go\\GOPATH"
    },
    "env_add_path": [
        ".g\\go\\bin",
        ".go\\GOPATH\\bin"
    ],
    "persist": [
        ".g",
        ".go"
    ],
    "checkver": "github",
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/voidint/g/releases/download/v$version/g$version.windows-amd64.zip"
            },
            "32bit": {
                "url": "https://github.com/voidint/g/releases/download/v$version/g$version.windows-386.zip"
            }
        }
    }
}
