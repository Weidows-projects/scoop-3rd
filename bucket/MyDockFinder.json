            "pre_install": [
                "move $dir\\MyDock\\* $dir",
                "rm $dir\\MyDock",
                "foreach ($f in @('config.ini', 'ico.ini', 'padconfig.xml')) {",
                "    if (-not (Test-Path \"$persist_dir\\$f\")) {",
                "        Write-Host 'File' $f 'does not exists. Creating' -f Yellow",
                "        Set-Content \"$dir\\$f\" $CONT -Encoding Ascii",
                "    }",
                "}"
            ]
        }
    },
