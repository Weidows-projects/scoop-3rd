{
    "version": "2.42.0.windows.2",
    "description": "Distributed version control system. (All .exes shimmed)",
    "homepage": "https://gitforwindows.org",
    "license": "GPL-2.0-only",
    "architecture": {
        "64bit": {
            "url": "https://github.com/git-for-windows/git/releases/download/v2.42.0.windows.2/PortableGit-2.42.0.2-64-bit.7z.exe#/dl.7z",
            "hash": "9254a4a0c597f1c4700cb607cedd83aef2215c289a3b2b1b7475cf58d51c005e"
        },
        "32bit": {
            "url": "https://github.com/git-for-windows/git/releases/download/v2.42.0.windows.2/PortableGit-2.42.0.2-32-bit.7z.exe#/dl.7z",
            "hash": "213e48d5d0e86f4cf2da2d5d1012e477d58ea03f16d7008b6751c32f731080c1"
        }
    },
    "bin": [
        "git-bash.exe",
        "cmd\\git.exe",
        "cmd\\gitk.exe",
        "cmd\\git-gui.exe"
    ],
    "env_add_path": [
        "usr\\bin"
    ],
    "shortcuts": [
        ["git-bash.exe", "Git Bash", "--cd-to-home"],
        ["cmd\\git-gui.exe", "Git GUI"]
    ],
    "checkver": {
        "github": "https://github.com/git-for-windows/git",
        "regex": "v([\\w.]+)/PortableGit-(?<ver>[\\d\\w.]+)-64-bit"
    },
    "env_set": {
        "GIT_INSTALL_ROOT": "$dir"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/git-for-windows/git/releases/download/v$version/PortableGit-$matchVer-64-bit.7z.exe#/dl.7z"
            },
            "32bit": {
                "url": "https://github.com/git-for-windows/git/releases/download/v$version/PortableGit-$matchVer-32-bit.7z.exe#/dl.7z"
            }
        },
        "hash": {
            "url": "https://github.com/git-for-windows/git/releases/latest",
            "regex": "<td>$basename</td>\\s*<td>$sha256</td>"
        }
    }
}
